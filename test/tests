// # To test the StringType class 

String s = “hello world!”
s.print()
String _foo_bar = "# This string has a # and the string literal has a _ "
_foo_bar.print()

StringType s1("Welcome to GPL ")
String s2 = s1.getValue()
s2.print()
String s3 = "This string is copied to the old string"
StringType s4(s3)
s4.getValue().print()
s4.print()
s2 = s4.substring(5)
s2.print()
s2 = s4.substring(5,21)
s2.print()
s3 = s2.concat("and now concatenated it to another string ")
s3.print()
number n1 = s4.find("to")
n1.print()
n1 = s4.findLast("String")
n1.print()
String s5 = s4.toUpper()
s5.print()
s5 = s4.toLower()
s5.print()
s5 = s4.reverse()
s5.print()

