print("")
Number i = 2
while (i)
    print("==================")
    i=i-1
    Number j = 2
    while (j)
        print(j)
        j=j-1
        Number k = 2
        while (k)
            k = k-1
            Number z = 2
            while (z)
                z=z-1
            end
            
            print(k)
        end
    end
    print("-----------------")
end

print("")
